{
  "name": "predikdev-monorepo",
  "private": true,
  "version": "0.0.0",
  "description": "Mono-repo for the portfolio.",
  "workspaces": [
    "web",
    "studio"
  ],
  "scripts": {
    "dev": "npm run dev --workspace web",
    "dev:studio": "npm run dev --workspace studio",
    "build": "npm run build --workspace web && npm run build --workspace studio",
    "build:web": "npm run build --workspace web",
    "build:studio": "npm run build --workspace studio"
  }
}
